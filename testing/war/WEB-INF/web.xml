<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Testing</servlet-name>
		<servlet-class>com.example.testing.TestingServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>student_insertion</servlet-name>
		<servlet-class>com.example.testing.student_insertion</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>student_validation</servlet-name>
		<servlet-class>com.example.testing.student_validation</servlet-class>
	</servlet>
	 <servlet>
        <servlet-name>testing1</servlet-name>
        <servlet-class>com.example.testing.testing1</servlet-class>
    </servlet>
	 <servlet>
        <servlet-name>sender</servlet-name>
        <servlet-class>com.example.testing.sender</servlet-class>
    </servlet>
  
    <servlet>
        <servlet-name>keyreceiver</servlet-name>
        <servlet-class>com.example.testing.keyreceiver</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admin_insertion</servlet-name>
        <servlet-class>com.example.testing.admin_insertion</servlet-class>
    </servlet>
    <servlet>
    	<servlet-name>Upload</servlet-name>
    	<servlet-class>com.example.testing.Upload</servlet-class>
  </servlet>
  
  <servlet>
    	<servlet-name>Serve</servlet-name>
   	 <servlet-class>com.example.testing.Serve</servlet-class>
  </servlet>
  <servlet>
        <servlet-name>Hui</servlet-name>
        <servlet-class>com.example.testing.Hui</servlet-class>
    </servlet>
    
      <servlet-mapping>
        <servlet-name>Testing</servlet-name>
        <url-pattern>/ts</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>student_insertion</servlet-name>
        <url-pattern>/student_insertion</url-pattern>
    </servlet-mapping>
     <servlet-mapping>
        <servlet-name>student_validation</servlet-name>
        <url-pattern>/student_validation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admin_insertion</servlet-name>
        <url-pattern>/admin_insertion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sender</servlet-name>
        <url-pattern>/sender</url-pattern>
    </servlet-mapping>
  
    <servlet-mapping>
        <servlet-name>keyreceiver</servlet-name>
        <url-pattern>/keyreceiver</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
		<servlet-name>testing1</servlet-name>
		<url-pattern>/testing1</url-pattern>
	</servlet-mapping>
	 <servlet-mapping>
   		 <servlet-name>Upload</servlet-name>
    		<url-pattern>/upload</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    	<servlet-name>Serve</servlet-name>
    	<url-pattern>/serve</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    	<servlet-name>Hui</servlet-name>
    	<url-pattern>/hui</url-pattern>
  </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
